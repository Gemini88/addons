<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.navi.x" name="Navi-X plugin"
	version="0.3.6" provider-name="cbb">
	<requires>
		<import addon="xbmc.python" version="1.0" />
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
	<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">Navi-X Media browser</summary>
		<description lang="en">Navi-X plugin with whufclee homepage</description>
	</extension>
	<statfeed>
		<working>yes</working>
		<notes>Works on all platform but some Navi-X functions may not work. The most notable is RSS feeds will not open</notes>
		<xbox>yes</xbox>
	</statfeed>	
</addon>

